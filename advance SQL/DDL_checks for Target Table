SELECT CONCAT(DATA_TYPE, ' : ', Count(*)) as DataType
		 FROM INFORMATION_SCHEMA.COLUMNS
		 WHERE TABLE_NAME ='vw_apprenticeship'
		 group by DATA_TYPE order by 1


